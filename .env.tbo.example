# TBO (Travel Boutique Online) Flight Supplier Configuration
# Add these to your Render environment variables or .env file

# ============================================================================
# TBO API Configuration
# ============================================================================
TBO_BASE_URL=https://tboapi.travelboutiqueonline.com/AirAPI_V10/AirService.svc/rest
TBO_AGENCY_ID=your_agency_id_here
TBO_END_USER_IP=192.168.5.56
TBO_CREDENTIAL_MODE=runtime
TBO_TIMEOUT_MS=15000

# ============================================================================
# TBO Authentication (Runtime Mode)
# Required if TBO_CREDENTIAL_MODE=runtime
# ============================================================================
TBO_CLIENT_ID=your_client_id_here
TBO_USERNAME=your_username_here
TBO_PASSWORD=your_password_here

# ============================================================================
# TBO Authentication (Static Mode)
# Required if TBO_CREDENTIAL_MODE=static
# Uncomment if using pre-generated token
# ============================================================================
# TBO_TOKEN_ID=your_static_token_here

# ============================================================================
# Multi-Supplier Configuration
# ============================================================================
FLIGHTS_SUPPLIERS=amadeus,tbo

# ============================================================================
# Existing Variables (Keep these)
# ============================================================================
# AMADEUS_API_KEY=your_amadeus_key
# AMADEUS_API_SECRET=your_amadeus_secret
# DATABASE_URL=postgresql://...
