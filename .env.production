# Production Environment Configuration
NODE_ENV=production
VITE_NODE_ENV=production

# API Configuration
API_BASE_URL=https://your-api-domain.com/api
VITE_API_BASE_URL=https://your-api-domain.com/api

# Security: Disable offline fallback in production
ENABLE_OFFLINE_FALLBACK=false
VITE_ENABLE_OFFLINE_FALLBACK=false

# Monitoring
SENTRY_DSN=your-sentry-dsn
VITE_SENTRY_DSN=your-sentry-dsn

# Database
DATABASE_URL=your-production-database-url

# External APIs
AMADEUS_API_KEY=your-production-amadeus-key
HOTELBEDS_API_KEY=your-production-hotelbeds-key
